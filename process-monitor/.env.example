# Process Monitor Environment Variables
# Copy this file to .env and update with your values

# Optional: Override monitoring interval (in seconds)
# MONITORING_INTERVAL=5

# Optional: Override CPU kill threshold (percentage)
# CPU_KILL_THRESHOLD=95.0

# Optional: Override memory kill threshold (percentage)
# MEMORY_KILL_THRESHOLD=90.0

# Optional: Disable notifications
# NOTIFICATIONS_ENABLED=false
